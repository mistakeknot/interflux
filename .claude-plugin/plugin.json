{
  "name": "interflux",
  "version": "0.2.8",
  "description": "Multi-agent review and research with scored triage, domain detection, content slicing, and knowledge injection. 17 agents (12 review + 5 research), 3 commands, 2 skills, 2 MCP servers. Companion plugin for Clavain.",
  "author": {
    "name": "mistakeknot"
  },
  "license": "MIT",
  "keywords": [
    "review",
    "research",
    "multi-agent",
    "flux-drive",
    "flux-research",
    "domain-detection",
    "triage",
    "code-review",
    "document-review",
    "exa"
  ],
  "mcpServers": {
    "qmd": {
      "type": "stdio",
      "command": "qmd",
      "args": [
        "mcp"
      ]
    },
    "exa": {
      "type": "stdio",
      "command": "npx",
      "args": [
        "-y",
        "exa-mcp-server"
      ],
      "env": {
        "EXA_API_KEY": "${EXA_API_KEY}"
      }
    }
  }
}
